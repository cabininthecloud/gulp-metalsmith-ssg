{
    "env": {
      "es6": true,
      "browser": true
    },
    "extends": ["airbnb-base", "plugin:prettier/recommended"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "no-plusplus": 0,
      "consistent-return": 0,
      "no-await-in-loop": 1,
      "import/named": 1
    }
  }